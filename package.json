{
  "name": "demeteorizer",
  "description": "Converts a Meteor app into a \"standard\" Node.js application.",
  "keywords": [
    "meteor"
  ],
  "version": "0.1.0",
  "author": "Modulus <support@modulus.io>",
  "maintainers": [
    "Brandon Cannaday <brandon@modulus.io>"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/OnModulus/demeteorizer"
  },
  "dependencies": {
    "commander" : "1.1.x",
    "async" : "0.2.x"
  },
  "devDependencies" : {
    "mocha" : "1.8.x"
  },
  "main": "./lib/demeteorizer",
  "bin" : {
    "meteor-normalizer": "./bin/demeteorizer"
  },
  "preferGlobal" : true,
  "engines": {
    "node": ">= 0.8.0"
  }
}
